<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/details_navigation_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/listFragment"
        android:name="com.prokarma.reference.architecture.feature.home.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/action_list_to_details"
            app:destination="@+id/detailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.prokarma.reference.architecture.feature.search.list.EventDetailsFragment"
        android:label="fragment_details"
        tools:layout="@layout/fragment_details">
        <argument
            android:name="event"
            android:defaultValue='""'
            app:argType="com.prokarma.reference.architecture.model.Event" />
    </fragment>
</navigation>